<idea-plugin>
    <id>com.sheepapps.vivabloc.vivabloc</id>
    <name>Viva Block</name>
    <version>1.0</version>
    <vendor email="hello@sheep-apps.com" url="https://sheep-apps.com/">Sheep Apps LLC</vendor>

    <description><![CDATA[
        Simplify Flutter BLoC pattern development
        ]]></description>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultProjectTypeProvider type="Android"/>
        <projectConfigurable
                instance="ui.settings.SettingsViewImpl">
        </projectConfigurable>
        <projectService serviceInterface="data.component.VivaBLoCComponent" serviceImplementation="data.component.VivaBLoCComponent"/>
    </extensions>

    <actions>
    </actions>
</idea-plugin>